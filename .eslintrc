{
  "extends": ["react-native"],
  "rules": {
      "no-console": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "react/prefer-stateless-function": 0,
      "class-methods-use-this": 0,
      "import/default": 0,
      "import/prefer-default-export": 0,
      "no-use-before-define": 1,
      "arrow-body-style": 0,
      "react-native/no-color-literals": 0,
      "react/jsx-handler-names": 0,
  }
}
